{
  "name": "pacoorozco/ssham",
  "description": "SSH Access Manager - a web based application to manage access to servers based on SSH keys.",
  "keywords": [
    "ssh",
    "ssh-key",
    "unix-accounts",
    "security",
    "keyrings",
    "access-management"
  ],
  "homepage": "https://github.com/pacoorozco/ssham",
  "license": "GPL-3.0",
  "type": "project",
  "authors": [
    {
      "name": "Paco Orozco",
      "email": "paco@pacorozco.info",
      "homepage": "https://pacoorozco.info",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/pacoorozco/ssham/issues"
  },
  "require": {
    "laravel/framework": "5.2.*",
    "laravelcollective/html": "5.2.*",
    "laravelcollective/remote": "5.2.*",
    "phpseclib/phpseclib": "^2.0",
    "yajra/laravel-datatables-oracle": "~6.0",
    "torann/registry": "0.2.*@dev",
    "felixkiss/uniquewith-validator": "2.*"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.0",
    "symfony/dom-crawler": "~3.0",
    "symfony/css-selector": "~3.0",
    "laravel/homestead": "^4.0",
    "barryvdh/laravel-ide-helper": "^2.2"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "SSHAM\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan ide-helper:generate",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
